    "lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore --fix src",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "pre-commit": "npm run build && npm run test:vitest -- --run && npm run test:pact && npm test -- -o",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:storybook": "test-storybook",
    "chromatic": "npx chromatic --debug --only-changed"
